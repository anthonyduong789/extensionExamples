{
  "manifest_version": 3,
  "name": "HTML Injector",
  "version": "1.0",
  "description": "Injects HTML into web pages",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": ["mypage.html", "content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]




}